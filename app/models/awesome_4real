mimetypes2 = [[1,'application/zip'], [2,'application/x-gzip'], [3,'application/x-tar'], [4,'text/plain']]
    mimetypes = Hash[*mimetypes2.flatten(1)]
    puts mimetypes
    puts  mimetypes.invert[filetype]
    cool =  mimetypes.invert[filetype]
    if cool.size > 0
      worked = save_upload_file(collection_params, uploaded_io)
      return filename
    else
      collection.errors.add_to_base("File upload was in the wrong file type format. </br></br> Please upload either tar, gzip, zip or plain text files")
      return false
    end
